<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/sumoConfiguration.xsd">

    <input>
        <net-file value="net2.net.xml"/>
        <route-files value="rou2.rou.xml"/>
    </input>
	
    <output>
        <summary-output value="test2.summary.xml" />
        <tripinfo-output value="test2-tripinfo.xml"/>
		<emission-output value="test2-emission.xml"/>
    </output>
	
    <time>
        <begin value="0"/>
        <end value="400"/>
    </time>
	
	<processing>
		<lateral-resolution value="0.02"/>
		<step-length value="0.2"/>
		<threads value="8"/>
	</processing>
	
	<simulation>
		<collision.action value="warn"/>
	</simulation>

</configuration>